{
    "name": "Drdre4life2/laravel-api-response",
    "description": "A simple JSON response helper for Laravel 10+",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dare Adewuyi",
            "email": "drdre4life2001@yahoo.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Drdre4life2\\ApiResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drdre4life2\\ApiResponse\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
